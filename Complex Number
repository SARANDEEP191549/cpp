#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a, b,c,d;
    cin>>a>>b>>c>>d;
    cout<<a+c<<endl;
    cout<<b+d<<endl;
    cout<<a-c<<endl;
    cout<<b-d<<endl;
    return 0;
}
